{
  "$schema": "https://turbo.build/schema.json",
  "extends": [],
  "globalDependencies": ["tsconfig.base.json", "package.json", "package-lock.json"],
  "pipeline": {
    "dev": {
      "cache": false,
      "dependsOn": ["^dev"]
    },
    "lint": {
      "dependsOn": ["^lint"],
      "outputs": []
    },
    "test": {
      "dependsOn": ["^test"],
      "outputs": []
    },
    "build": {
      "dependsOn": ["^build"],
      "outputs": [".next/**", "dist/**", "build/**"]
    },
    "clean": {
      "cache": false
    }
  }
}
